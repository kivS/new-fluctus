{
  "name": "fluctus",
  "version": "0.1.7",
  "description": "Video and audio media in a floating window",
  "main": "main.js",
  "author": "Vik Borges",
  "license": "MIT",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-dmg": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "electron-nightly": "^19.0.0-nightly.20220209"
  },
  "dependencies": {
    "@sentry/electron": "^3.0.3",
    "electron-squirrel-startup": "^1.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "protocols": [
          {
            "name": "Fluctus Launch Protocol",
            "schemes": ["fluctus"]
          }
        ],
        "osxSign": {
          "identity": "Developer ID Application: Vik Borges (3XHWAYK6RW)",
          "hardened-runtime": true,
          "entitlements": "entitlements.plist",
          "entitlements-inherit": "entitlements.plist",
          "signature-flags": "library",
          "gatekeeper-assess": false
        },
        "osxNotarize": {
          "appleId": "v1k@protonmail.com",
          "appleIdPassword": "avsx-thpu-ttsl-icbb"
        }
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "fluctus"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": ["darwin"]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
